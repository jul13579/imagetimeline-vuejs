{
  "name": "timeline-vuejs",
  "version": "1.0.4",
  "main": "dist/timeline-vuejs.common.js",
  "description": "Minimalist Timeline with VueJS",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name timeline-vuejs ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "keywords": [
    "timeline",
    "vue-timeline",
    "timeline-vuejs",
    "minimalist timeline",
    "timeline components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jul13579/timeline-vuejs"
  },
  "bugs": {
    "url": "https://github.com/jul13579/timeline-vuejs/issues"
  },
  "homepage": "https://github.com/jul13579/timeline-vuejs",
  "author": "Pablo <psirera4@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-prettier": "^3.0.1",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
